#include <stdio.h>
 
long int Fibbonachi(long int number)
{

    if (number == 0) 
    {
      return 0;
    }

    if (number == 1 || number == 2)
    {
      return 1;
    } 
  
  return Fibbonachi(number - 1) + Fibbonachi(number - 2);
}
 
int main(void)
{
  long int number;

  scanf("%li", &number);
  printf("%li", Fibbonachi(number));

}
